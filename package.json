{
  "name": "project-npmPackageTypescript",
  "version": "1.0.0",
  "description": "package npm typescript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "jest": "jest --config jestconfig.json",
    "build": "tsc -p .",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm jest && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test:tsc": "tsc -p tsconfig.test.json -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abrahamibo/project-npmPackageTypescript.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abrahamibo/project-npmPackageTypescript/issues"
  },
  "homepage": "https://github.com/abrahamibo/project-npmPackageTypescript#readme",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
}
